'''
This codeblock used to find  "prime number"
@Cai
'''
#
def prime_number(n): 
    l=list()
    for i in range(1,n+1):
        
        if n%i==0:
            l.append(i)
    if len(l)==2:
        return n
 


n=1000
for i in range(2,n):
    #print(i)
    if prime_number(i):
        a=2**i-1
        #print(a)
        if prime_number(a):
            b=2**(i-1)
            #print(b)
            print(a*b)
